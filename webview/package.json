  "private": true,
  "dependencies": {
    "antd": "^4.11.1",
    "d3": "^5.16.0",
    "d3-context-menu": "^2.1.0",
    "react": "^17.0.1",
    "react-css-theme-switcher": "^0.2.2",
    "react-dom": "^17.0.1",
    "react-measure": "^2.5.2",
    "ts-debounce": "^2.2.0",
    "webcola": "^3.4.0"
  },
  "devDependencies": {
    "@types/d3": "^5.16.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-measure": "^2.0.6",
    "autoprefixer": "^10.2.3",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^9.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "postcss": "^8.2.4",
    "react-scripts": "5.0.1",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "buildAndCopy": "npm run build && npm run copyBuild && npm run copyThemes",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "copyBuild": "cp build/static/js/main*.js ../dist/webview.js.bin && cp build/static/css/main*.css ../dist/webview.css.bin",
    "copyThemes": "cp build/dark-theme.css ../dist/dark-theme.css.bin && cp build/light-theme.css ../dist/light-theme.css.bin"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}